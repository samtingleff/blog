#!/bin/sh
rm -rf index/*
java -jar /Users/stingleff/workspace/yassg/yassg-static/target/yassg-static-1.0.0-jar-with-dependencies.jar -index index/ -alchemyUsername $ALCHEMY_USERNAME -alchemyPassword $ALCHEMY_PASSWORD -alchemyCache alchemy-cache/ -content content/ -templates templates/ -output output/
rsync -av static/ output/
